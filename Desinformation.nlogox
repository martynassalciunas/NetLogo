                                           ;; 1~5 (worker), 6 (unemployed), 7 (retired), 8 (pupil)
  
  let dem6_c map [row -> item 88 row] db_c ;; Income
                                           ;; 1 (<500 eur), 2 (<1,000), 3 (<1,500), 4 (<2,000), 5 (<3,000), 6 (<4,000), 7 (4,000+)
  
  let dem7_c map [row -> item 89 row] db_c ;; Marital status
                                           ;; 1 (single/unmarried), 2 (married/unregistered), 3 (other)
  
  let dem8_c map [row -> item 90 row] db_c ;; Location
                                           ;; 1 (major cities), 2 (other cities/regional centers), 3 (rural)
  
  let temp randomize (
    list id_c trait1_c trait2_c trait3_c trait4_c trait5_c trait6_c trait7_c trait8_c trait9_c trait10_c trait11_c dem1_c dem2_c dem3_c dem4_c dem5_c dem6_c dem7_c dem8_c disin1_c disin2_c disin3_c disin4_c disin5_c disin6_c disin7_c disin8_c disin9_c disin10_c disin11_c disin12_c disin13_c disin14_c disin15_c disin16_c disin17_c disin18_c disin19_c disin20_c tech1_c tech2_c tech3_c tech4_c tech5_c tech6_c tech7_c tech8_c tech9_c tech10_c tech11_c
