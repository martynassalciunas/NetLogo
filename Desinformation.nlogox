;; Agents ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to-report find-index [lst num]
  if item num lst > 0 [ report num ]
  report find-index lst (num + 1)
end

to-report mod-list [lst idx]
  let num (item idx lst - 1)
  report replace-item idx lst num
end

to agent-creation
  set shape "circle"
  set size 3
  
  set soc-capital [0 0 0 0]
  set personality []
  
  let i who
  set traits map [col -> item i col] db_c_traits
  set demo map [col -> item i col] db_c_demo
  set tech map [col -> item i col] db_c_tech
  set disin map [col -> item i col] db_c_disin
  set narrative -1
  
  repeat dimension-count [
    set personality fput (random-float 10 - 5) personality
  ]
end

to-report find-index-val [lst val much]
  let nlst []
  let idx 0
  
  foreach lst [
    x -> if x = val [
      set much (much - 1)
      set nlst fput idx nlst
      if much = 0 [
        report nlst
      ]
    ]
    set idx (idx + 1)
  ]
  
  report nlst
end

to setup-agents
  if centralise-locations = true [ 
    let i 0
    repeat seedcount [
      create-turtles 1 [
        agent-creation
        
        set physical-x (random-float (physical-world-scale - 20)) + 10
        set physical-y (random-float (physical-world-scale - 20)) + 10
        set physical (list physical-x physical-y)
        
        set color 50 - i * 9
        set tempcolor 50 - i * 9
      ]
      set i (i + 1)
    ]
    
    ;; Create a random distribution of central agent towns
    let ranlist []
    repeat seedcount [
      set ranlist fput 0 ranlist
    ]
    repeat (agentcount - seedcount) [
      let rng (random seedcount)
      let num (item rng ranlist + 1)
      set ranlist replace-item rng ranlist num
    ]
    
    repeat (agentcount - seedcount) [
      create-turtles 1 [
        agent-creation
        
        let rng random-float 1
        let idx (find-index ranlist 0)
        set ranlist (mod-list ranlist idx)
        set color 50 - idx * 9
        set tempcolor 50 - idx * 9
        
        set rng (random-float 5)
        let angle (random-float 360)
        let close-x [physical-x] of (turtle idx)
        let close-y [physical-y] of (turtle idx)
        set physical-x (close-x + (rng * cos angle))
        set physical-y (close-y + (rng * sin angle))
        set physical (list physical-x physical-y)
      ]
    ]
  ]
  
  if centralise-locations = false [
    repeat agentcount [
      create-turtles 1 [
        agent-creation
        
        set physical-x (random-float physical-world-scale)
        set physical-y (random-float physical-world-scale)
        set physical (list physical-x physical-y)
        
        set color 55
        set tempcolor 55
      ]
    ]
  ]

  if anyone-broadcasts = false [
    set broadcaster-agents (find-index-val (item 4 db_c_traits) 5 broadcasters)
  ]
  
  if anyone-broadcasts = true [
    let templist []
    let i 0
    repeat agentcount [
      set templist lput i templist
      set i (i + 1)
    ]
    set broadcaster-agents templist
  ]
    
  ask turtles [
    let other-turtles other turtles
    let closest-physical-turtles min-n-of interaction-amount other turtles [
      physical-distance self myself
    ]
    set neighbours closest-physical-turtles
  ]
end

;; Network ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to setup-field
  ask patches [
    set pcolor white
  ]
end

to-report physical-distance [ self-agent other-agent ]
  let dist 0
  let self-physical [physical] of self-agent
  let other-physical [physical] of other-agent
  
  report sum (map [ [a b] -> (a - b) * (a - b) ] self-physical other-physical)
end

to-report virtual-distance-local [ self-virtual other-virtual ]
  report sum (map [ [a b] -> (a - b) * (a - b) ] self-virtual other-virtual)
end

to-report collect-max [ vc ]
  let vals map [ x -> item 1 x ] vc
  let m max vals
  report (list m position m vals)
end

to-report personality-distance [ self-agent other-agent virtual-candidates ]
  let dist 0
  let i 0
  let self-virtual [personality] of self-agent
  let other-virtual [personality] of other-agent
  let len length self-virtual
  
  let cmaxidx collect-max virtual-candidates
  let cmax (first cmaxidx)
 
;   set dist sum (map [
;     [a b] -> (a - b) ^ 2
;   ] self-virtual other-virtual)
  
  while [i < len] [
    let a (item i self-virtual)
    let b (item i other-virtual)
    let c (a - b)
    set dist dist + ( c * c )
    if dist > cmax [ report virtual-candidates ]
    set i (i + 1)
  ]
  
  report replace-item (last cmaxidx) virtual-candidates (list other-agent dist)
end

to-report desin-infect-chance [ text-id agent-personality ]
  let text-disin (item text-id (item 0 db_b))
  let text-tech (item text-id (item 1 db_b))
  
  let total-tech-sens 0
  let total-disin-sens 0
  let i 0

  ;; Note a limitation here that the perceived sensitivity toward a topic is depersonalised, it is impossible to discern in the given data how much is "strong agreement" for every person individually 
  
  foreach text-tech [
    x -> 
    set total-tech-sens (total-tech-sens + (x * (item i tech) / 5))
    set i (i + 1)
  ]
  set i 0
  
  foreach text-disin [  ;; topic of the text
    x -> 
    set total-disin-sens (total-disin-sens + (x * (item i disin) / 10))
    set i (i + 1)
  ] 
  
  report total-tech-sens * total-disin-sens
end

to disinformation-processing-logic [ text-id other-agent-personality ]
  ;; item n tech  -> Nth turtle nth item of the db_c_tech  to denote topic sensitivity
  ;; item n desin -> Nth turtle nth item of the db_c_desin to denote knowledge of desinformation tactic
  
  let rng random-float 1
  
  if (desin-infect-chance text-id other-agent-personality) > rng [
    
    let personality-diff virtual-distance-local personality other-agent-personality
    let likemindedness (1 - min list 1 (personality-diff / (s_max * decaying-strength)))
    let deg-of-effect (interaction-strength * likemindedness)
    
    set personality (
      map [[a b] ->
        (a * (1 - deg-of-effect) ;; XX% of current personality 
          + ;; = 100%
          (b * deg-of-effect)) ;; XX% of adapted personality
      ]
      personality other-agent-personality )
    
    let dbbsoc item text-id (item 2 db_b)
    set soc-capital (map [ [a b] -> max (list -1 (a + ((b / 5) * deg-of-effect))) ] soc-capital (list (item 5 dbbsoc) (item 6 dbbsoc) (item 7 dbbsoc) (item 8 dbbsoc)))
    set traits replace-item 10 traits max (list -1 (min (list 1 ((item 10 traits) + ((item 2 dbbsoc) / 5 * deg-of-effect)))))
    ;; There should be a better way to calculate this?
    
;   let spp1 map [row -> item 13 row] db_a ;; Social polarity
;   let spp2 map [row -> item 6 row] db_a  ;; Destructive criticism
;   let spp3 map [row -> item 14 row] db_a ;; Social radicalization
;   let spp4 map [row -> item 9 row] db_a  ;; Information bubbles
;   let spp5 map [row -> item 5 row] db_a  ;; Democratic
;   let skn1 map [row -> item 15 row] db_a ;; Trust and cooperation
;   let skn2 map [row -> item 2 row] db_a  ;; Civic activism
;   let skn3 map [row -> item 12 row] db_a ;; Social network support
;   let skn4 map [row -> item 11 row] db_a ;; Personal connections
    
    set narrative text-id
  ]
end

to agentlogic
  
  ;; fix so OTHER agents get affected, not the agent itself
  
  let rng random-float 1
  
  let rng-int random 1016 ;; number of propaganda texts
  
  if broadcast-chance * broadcasters > rng [ 
    let broadcaster-personality []
    
    ask one-of turtles with [member? who broadcaster-agents] [
      set broadcaster-personality [personality] of self
    ]
    
    ask other turtles [
      
      if (virtual-distance-local personality broadcaster-personality) < s_max * broadcaster-range-mult  [ ; 8^d hyperspheres would tile in hypercubic arrangement I think
        disinformation-processing-logic rng-int broadcaster-personality
      ]
      
      ;; Am I tracking soc capital correctly ?
      
      ;; Could run individual theme tests per disinformation type
      ;; Direct trait modification based on social capital ?
      
    ]
  ]
  
  ask turtles [
    ;; Let agents find their own disinformation
    let nartemp random 1016
    set rng random-float min-of-disin
    
    ;; Making an assumption that those who spend an hour a day browsing social media are guaranteed to see desinformation
    ;; Combined with resilience of fact-checking and resilience to a topic, and the spreading odds
    if (item 0 traits) * (1 - (item 1 traits) / 5) * (desin-infect-chance nartemp personality) * (1 - (item 5 traits) / 5) * (1 - (item 6 traits) / 5) > rng [
      set narrative nartemp
    ]
    
    if narrative != -1 [
      ;; Choose a neighbour to talk to
      if neighbour-interaction = true [
        
        let orig-personality personality
        
        set rng random-float 1
        
        let chosen-one one-of neighbours
        
        ask chosen-one [
          if narrative != -1 and (1 - (item 1 traits) / 5) * interaction-probability > rng [ ;; Ensure there's a narrative to talk about
            disinformation-processing-logic narrative orig-personality
            if (item 10 traits) > (random-float 1) [
              set narrative -1 ;; talked about it
            ]
          ]
        ]
      ]
      
      ;; Choose agents through personality (virtual) interaction
      if virtual-interaction = true [
        
        let orig-personality personality
        
        let virtual-candidates []
        repeat interaction-amount [
          set virtual-candidates fput (list nobody s_max) virtual-candidates
        ]
        
        ask other turtles [
          set virtual-candidates (personality-distance myself self virtual-candidates)
        ]
        
        ;; collect turtles from list
        let i 0
        foreach virtual-candidates [
          x ->
          set virtual-candidates (replace-item i virtual-candidates (item 0 x))
          set i (i + 1)
        ]
        
        ask one-of virtual-candidates [
          set rng random-float 60
          set rng-int random 1016
          
          if narrative != -1 and (item 0 traits) * (1 - (item 1 traits) / 5) * interaction-probability > rng [
            disinformation-processing-logic narrative orig-personality
            if (item 10 traits) > (random-float 1) [
              set narrative -1 ;; talked about it
            ]
          ]
        ]
      ]
    ]
    
    setxy ( calc-position-x item x-axis-feature personality ) ( calc-position-y item y-axis-feature personality )
  ]
end

;; Visualisation ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to drawtrail
  let c 80
  
  ask turtles [
    if ((highlight-demographic > 0 or highlight-central > 0) and color = 15) or (highlight-demographic = 0 and highlight-central = 0) [
      set pcolor 5 + c
    ]
  ]
  
  if ticks mod (200 / trail-decay-speed) = 0 [
    ask patches [
      if pcolor = 9 + c [ set pcolor 9.9 ]
      if pcolor = 8 + c [ set pcolor pcolor + 1 ]
      if pcolor = 7 + c [ set pcolor pcolor + 1 ]
      if pcolor = 6 + c [ set pcolor pcolor + 1 ]
      if pcolor = 5 + c [ set pcolor pcolor + 1 ]
    ]
  ]
end

to-report calc-position-x [l]
  report ((world-width - 1) * 0.1) * l + (world-width - 1) / 2
end

to-report calc-position-y [l]
  report ((world-height - 1) * 0.1) * l + (world-height - 1) / 2
end

to updview
  setxy ( calc-position-x item x-axis-feature personality ) ( calc-position-y item y-axis-feature personality )
end

to colorupd
  set color tempcolor
  
  if highlight-demographic > 0 [
    if item demo-trait demo = highlight-demographic [
      set color 15
    ]
    if item demo-trait demo != highlight-demographic [
      set color 55
    ]
  ]
  
  if highlight-central > 0 [
    if tempcolor = 50 - highlight-central * 9 [
      set color 15
    ]
    if tempcolor != 50 - highlight-central * 9 [
      set color 55
    ]
  ]
end

to-report calc-physical-position-x [px]
  report ((world-width - 1) / physical-world-scale) * px
end

to-report calc-physical-position-y [py]
  report ((world-height - 1) / physical-world-scale) * py
end

to showphysical
  ask turtles [
    setxy (calc-physical-position-x physical-x) (calc-physical-position-y physical-y)
    colorupd
  ]
  
  ask turtle 0 [
    updview
  ]
end

to go ;; The only difference's they're "forever" and not
  agentlogic
  drawtrail
  
  tick
end

to step
  agentlogic
  drawtrail
  
  tick
end]]></code>
