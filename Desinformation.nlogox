                                           ;; 1 (18-25), 2 (26-35), 3 (36-45), 4 (46-55), 5 (56-65), 6 (65+)
  
  let dem2_c map [row -> item 84 row] db_c ;; Gender
                                           ;; 1 (male), 2 (female), 3 (unknown)
  
