                                             ;; distrustOfWesternInstitutions, warInUkraine, endOfWashington
  
  let disin13_c map [row -> item 75 row] db_c ;; Russian economy is better from regulations
                                             ;; endOfWesternCivilization, newWorldOrder. lithuanianDefamation
  
  let disin14_c map [row -> item 76 row] db_c ;; Russian culture is better protected
                                             ;; endOfWesternCivilization, newWorldOrder, lithuanianDefamation
  
  let disin15_c map [row -> item 77 row] db_c ;; Self-governance is reduced from international - UN - regulations
                                             ;; endOfWesternCivilization, distrustOfWesternInstitutions, distrustOfLithuanianInstitutions
  
  let disin16_c map [row -> item 78 row] db_c ;; Baltic voting system is flawed
